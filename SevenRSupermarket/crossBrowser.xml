<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CrossBrowser">
	<listeners>
		<listener class-name="generate_Report.Listeners"></listener>
	</listeners>
	<test name="chromeTest">
		<parameter name="browzer" value="chrome"></parameter>
		<classes>
			<class name="test.ManageCategoryTest"></class>
		</classes>
	</test>
	<test name="edgeTest">
		<parameter name="browzer" value="edge"></parameter>
		<classes>
			<class name="test.ManageCategoryTest"></class>
		</classes>
	</test>
	<test name="firefoxTest">
		<parameter name="browzer" value="firefox"></parameter>
		<classes>
			<class name="test.HomePageTest"></class>
		</classes>
	</test>
</suite>